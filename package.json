  "dependencies": {
    "axios": "0.16.2",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-redux": "5.0.5",
    "react-router-dom": "4.1.1",
    "react-scripts": "1.0.10",
    "redux": "3.7.0",
    "redux-thunk": "2.2.0",
    "semantic-ui-css": "2.2.10",
    "semantic-ui-react": "0.71.0",
    "validator": "8.0.0"
